{
    "name": "Colorize The Logs",
    "author": "Joshua Wilborn",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Re-add terminal colors to AWS CodeBuild Logs and delete control characters.",
    "content_scripts": [
        {
            "matches": [
                "https://console.aws.amazon.com/codesuite/codebuild/projects/*/build/*"
            ],
            "js": [
                "constants.js",
                "colorize-the-logs.js"
            ],
            "run_at": "document_start"
        }
    ]
}